<h2 style="color:crimson; font-weight: bold;">Registros cargados</h2>

<% if(listaProductos.length> 0){ %>
    <div class="table-responsive">
        <table class="table table-dark">
            <tr style="color: yellow;">
                <th>Nombre</th>
                <th>Apellido</th>
                <th>Edad</th>
            </tr>
            <% for (const listaProducto of listaProductos) {%>
                <tr>
                    <td>
                        <%=listaProducto.name%>
                    </td>
                    <td>
                        <%=listaProducto.edad%>
                    </td>
                    <td>
                        <%=listaProducto.genero%>
                    </td>
                    <td>
                        <%=listaProducto.celular%>
                    </td>
                </tr>
                <%};%>
        </table>
    </div>
    <% } else{ %>
        <h3 class="alert alert-danger">no se encontraron datos</h1>
            <% } %>