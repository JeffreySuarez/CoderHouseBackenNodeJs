<h2 style="color:crimson; font-weight: bold;">Registros cargados</h2>
{{#if listaProductos.error }}
        <h3 class="alert alert-danger">no se encontraron datos</h1>
            {{else}}
    <div class="table-responsive">
        <table class="table table-dark">
            <tr style="color: yellow;">
                <th>Nombre</th>
                <th>Apellido</th>
                <th>Edad</th>
            </tr>
            <tbody>
                {{#each listaProductos}}
                <tr>
                    <td>
                        {{this.name}}
                    </td>
                    <td>
                        {{this.edad}}
                    </td>
                    <td>
                        {{this.genero}}
                    </td>
                    <td>
                        {{this.celular}}
                    </td>
                </tr>
                {{/each}}
                </tbody>
        </table>
    </div>
    {{/if}}